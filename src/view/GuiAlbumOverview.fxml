                  <Button id="guiAlbumOverviewAlbumBearbeiten" fx:id="guiAlbumOverviewAlbumBearbeiten" mnemonicParsing="false" onAction="#handleButtonAction" text="Album bearbeiten" textAlignment="CENTER" />
